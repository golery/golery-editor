{
  "name": "golery-editor",
  "private": true,
  "version": "2.0.0",
  "description": "Golery editor",
  "homepage": "https://www.golery.com",
  "author": {
    "name": "Golery",
    "email": "golery.team@gmail.com",
    "url": "https://www.golery.com"
  },
  "scripts": {
    "delete_build.dev": "webpack --config webpack.config.js --mode development --progress",
    "delete_build.prod": "webpack --config webpack.config.js --mode production  --progress",
    "delete_build": "yarn run build.dev && yarn run build.prod",
    "dev": "webpack serve --config webpack.config.sandboxbrowser.js --mode development --progress",
    "delete_watch.node": "webpack --config webpack.config.sandboxnode.js --mode development --progress --watch",
    "delete_dev.node": "node ./src/sandbox/build/server.js",
    "build": "rollup --config ./rollup.config.js",
    "watch": "rollup -w --config ./rollup.config.js"
  },
  "files": [
    "dist"
  ],
  "module": "dist/index.es.js",
  "dependencies": {
    "is-url": "^1.2.4",
    "prismjs": "^1.24.1",
    "slate": "^0.63.0",
    "slate-history": "^0.62.0",
    "slate-hyperscript": "^0.62.0",
    "slate-react": "^0.65.2"
  },
  "peerDependencies": {},
  "devDependencies": {
    "@rollup/plugin-typescript": "^8.2.5",
    "@types/react": "^17.0.11",
    "@types/react-dom": "^17.0.8",
    "css-loader": "^5.2.6",
    "node-sass": "^6.0.1",
    "path": "^0.12.7",
    "postcss": "^8.3.6",
    "postcss-loader": "^6.1.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "rollup": "^2.56.1",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-postcss": "^4.0.0",
    "rollup-plugin-typescript2": "^0.30.0",
    "sass": "^1.35.1",
    "sass-loader": "^12.1.0",
    "style-loader": "^3.0.0",
    "ts-loader": "^9.2.3",
    "typescript": "^4.3.4",
    "webpack": "^5.40.0",
    "webpack-cli": "^4.7.2",
    "webpack-dev-server": "^3.11.2"
  },
  "license": "MIT"
}
